{% props height = "", width = "", large = "", medium = "", small = "", alt = "" %}

<picture>
	<source srcset="{{ asset(large) }}" media="(min-width: 769px)"/>
	<source srcset="{{ asset(medium) }}" media="(min-width: 641px)"/>
	<source srcset="{{ asset(small) }}"/>
	<img class="object-cover {{ height }} {{ width }}" src="{{ asset(large) }}" alt="{{ alt }}"/>
</picture>
